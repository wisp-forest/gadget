    "text.config.gadget.option.dumpSafety": "Dump Safety",
    "text.config.gadget.option.dumpSafety.tooltip": [
        "Describes the precautions taken to avoid missed packets in dumps.\n\n",
        "- ", {"text": "On Pause\n", "bold": true},
        "  Only flushes when the pause menu is opened.\n",
        "  Not recommended.\n",
        "- ", {"text": "On JVM Exit\n", "bold": true},
        "  Flushes when the JVM exits cleanly. Results in \n",
        "  clean dump files, but may not always trigger.\n",
        "- ", {"text": "After Every Packet\n", "bold": true},
        "  Flushes on JVM exit and after every written packet.\n",
        "  Might result in degraded performance.\n"
    ],
    "text.config.gadget.enum.dumpSafetyMode.on_pause": [
        "On Pause"
    ],
    "text.config.gadget.enum.dumpSafetyMode.on_exit": [
        "On JVM Exit"
    ],
    "text.config.gadget.enum.dumpSafetyMode.after_every_packet": [
        "After Every Packet"
    ],
